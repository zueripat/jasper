{
    "root": true,
    "extends": ["eslint:recommended"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": "./tsconfig.json",
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "rules": {
        "no-unused-vars": 0,
        "no-undef": 0,
        "func-names": 0,
        "no-eval": 2,
        "no-invalid-this": 0,
        "no-return-await": 1,
        "no-var": 0,
        "require-await": 0,
        "no-extra-semi": 1,
        "prefer-const": 2,
        "ban-ts-comment": 0,
        "no-prototype-builtins": 1
    }
}
