{
    "name": "jasper",
    "version": "2.0.0",
    "description": "An open source private utility Discord bot",
    "scripts": {
        "build": "rimraf dist && npm run prettier && tsc",
        "start": "npm run build && npm run lint && node dist/index.js",
        "lint": "eslint src --ext .ts",
        "lint:fix": "eslint src --ext .ts --fix",
        "prettier": "prettier --write **/*.{ts,js,json}",
        "build-docker": "docker compose -p jasper build",
        "start-docker": "docker compose -p jasper up -d",
        "stop-docker": "docker compose -p jasper down"
    },
    "main": "dist/index.js",
    "private": true,
    "author": "zueripat <ruedepatrick@gmail.com>",
    "dependencies": {
        "@prisma/client": "^5.13.0",
        "cron": "^3.1.7",
        "discord.js": "^14.15.2",
        "dotenv": "^16.4.5",
        "winston": "^3.13.0"
    },
    "devDependencies": {
        "@types/eslint": "^8.56.10",
        "@types/node": "^20.12.12",
        "@typescript-eslint/eslint-plugin": "^7.9.0",
        "@typescript-eslint/parser": "^7.9.0",
        "rimraf": "^5.0.7",
        "typescript": "^5.4.5"
    }
}
